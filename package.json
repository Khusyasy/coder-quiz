{
  "name": "coder-quiz-server",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "start": "node ./bin/www",
    "dev": "concurrently \"nodemon ./bin/www\" \"cd ./client && npm run serve\"",
    "build": "npm i && cd ./client && npm i --only=dev && npm i && npm run build"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^9.0.1",
    "express": "~4.16.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.7",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "concurrently": "^6.1.0",
    "nodemon": "^2.0.7"
  }
}
